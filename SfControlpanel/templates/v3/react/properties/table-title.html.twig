import React, { Fragment } from 'react'
import { Th } from '@newageerp/ui.table.base.table'

{% set textAlignment = 'text-left' %}
{% if naeType == 'float' or naeType == 'float4' or naeType == 'number' or naeType == 'seconds-to-time'}
    {% set textAlignment = 'text-right' %}
{% endif %}

export const {{ compName }} = () => {
    return <Th textAlignment="{{textAlignment}}">{'{{ property.title }}'}</Th>
}