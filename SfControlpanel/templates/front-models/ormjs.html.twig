import { ORM } from "redux-orm";
{% for model in models %}
import {{ model }}Model from "./{{ model }}Model"
{% endfor %}

export const orm = new ORM({
    stateSelector: (state) => {
        return state.orm; // wherever the reducer is put during createStore
    },
});
orm.register(
{% for model in models %}
    {{ model }}Model,
{% endfor %}
);

export default orm;