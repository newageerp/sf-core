import { createSelector } from "redux-orm";
import orm from "./orm";

{% for m in models %}
export const Selector{{ m }}Nae = createSelector(orm.{{ m }}Model);
{% endfor %}

export const selectorBySchemaClassName = (schema) => {
    {% for m in models %}
    if (schema === '{{ m }}') {
        return Selector{{ m }}Nae;
    }
    {% endfor %}
}

export const selectorBySchemaSlug = (schema) => {
    {% for m in modelsSlugs %}
    if (schema === '{{ m.slug }}') {
        return Selector{{ m.cl }}Nae;
    }
    {% endfor %}
}